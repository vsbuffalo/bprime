{
  "name": "bmap_hg38_reps",
  "runtype": "samples",
  "slim": "segment.slim",
  "model": "bgs_segment",
  "params":
     {
       "N": {"dist": {"name": "fixed", "val": 1000}, "type": "int"},
       "mu": {"dist": {"name": "log10_uniform", "low": -10, "high": -7}, "type": "float"},
       "sh": {"dist": {"name": "log10_uniform", "low": -5, "high": -1}, "type": "float"},
       "rf": {"dist": {"name": "log10_uniform", "low": -10, "high": -0.824}, "type": "float"},
       "rbp": {"dist": {"name": "log10_uniform", "low": -12, "high": -7}, "type": "float"},
       "L": {"dist": {"name": "discrete_uniform", "low": 1, "high": 10000}, "type": "int"}
     },
  "nsamples": 100000,
  "nreps": 50,
  "average": true,
  "activs": ["elu"],
  "outactivs": ["sigmoid"],
  "fit_nreps": 4,
  "weight_l2": ["None", 0.00001, 1e-6, 1e-7],
  "bias_l2": ["None", 0.00001, 1e-6, 1e-7],
  "archs": [
            {"n8": 0, "n4": 0, "n2": 0, "nx": 1},
            {"n8": 0, "n4": 1, "n2": 0, "nx": 0},
            {"n8": 0, "n4": 2, "n2": 0, "nx": 0},
            {"n8": 0, "n4": 1, "n2": 1, "nx": 0},
            {"n8": 0, "n4": 0, "n2": 2, "nx": 0}
           ]
}
